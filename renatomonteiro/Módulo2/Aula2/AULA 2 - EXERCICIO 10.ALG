Algoritmo "aula 2 - exercicio 8"

Var
     a, b, c,: inteiro

Inicio
      escreval("digite um numero para a: ")
      leia(a)
      escreval("digite um numero para b: ")
      leia(b)
      escreval("a = ", a, ", b = ", b)
      c <- a
      a <- b
      b <- a
      escreval("a = ", a, ", b = ", b)


      
Fimalgoritmo